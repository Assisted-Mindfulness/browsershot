{
    "name": "assisted-mindfulness/browsershot",
    "description": "Convert a webpage to an image or pdf using headless Chrome",
    "homepage": "https://github.com/spatie/browsershot",
    "keywords":
    [
        "convert",
        "webpage",
        "image",
        "pdf",
        "screenshot",
        "browser",
        "chrome",
        "headless"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Chernyaev",
            "email": "bliz48rus@gmail.com",
            "homepage": "https://tabuna.github.io",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://github.com/freekmurze",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/image": "^2.2",
        "spatie/phpunit-snapshot-assertions": "^4.0",
        "spatie/temporary-directory": "^2.1",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AssistedMindfulness\\Browsershot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssistedMindfulness\\Browsershot\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
